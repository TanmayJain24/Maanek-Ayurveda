            <h2 class="ayur-section-title text-center" data-aos="fade-up" data-aos-duration="800">Our Ayurvedic Services</h2>
            <p class="text-center ayur-mb-lg" style="font-size: 20px; font-weight: bold;" data-aos="fade-up" data-aos-delay="200" data-aos-duration="800">
                Experience personalized treatments tailored to your unique constitution (Prakriti)
            </p>
            <div class="row">
                <% services.slice(0, 3).forEach((service, index) => { %>
                <div class="col-md-4 ayur-mb-md" data-aos="zoom-in-up" data-aos-delay="<%= 300 + index * 200 %>" data-aos-duration="900">
                    <div class="ayur-card">
                    <img src="<%= service.heroImage %>" alt="<%= service.title %>" class="ayur-card-img">
                    <div class="ayur-card-body text-center">
                        <span class="badge bg-primary ayur-mb-md"><%= service.category %></span>
                        <h4><%= service.title %></h4>
                        <p><%= service.description.substring(0, 100) %>...</p>
                        <a href="/services/<%= service.slug %>" class="ayur-btn ayur-btn-outline-dark ayur-mt-sm">Read More</a>
                    </div>
                    </div>
